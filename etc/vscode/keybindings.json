// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "cmd+l",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "cmd+d",
    "command": "-editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "cmd+d",
    "command": "workbench.action.splitEditor"
  },
  {
    "key": "ctrl+alt+cmd+[IntlYen]",
    "command": "-workbench.action.splitEditor"
  }
]